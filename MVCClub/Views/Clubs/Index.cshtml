@model IEnumerable<MVCClub.Club>

@{
    ViewBag.Title = "List Of Clubs";
}

<h2>List Of Clubs</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClubName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OpenDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemberShip.MembershipID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.President.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Center.Location)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ClubName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OpenDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemberShip.MembershipID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.President.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Center.Location)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ClubID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ClubID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ClubID })
        </td>
    </tr>
}

</table>
